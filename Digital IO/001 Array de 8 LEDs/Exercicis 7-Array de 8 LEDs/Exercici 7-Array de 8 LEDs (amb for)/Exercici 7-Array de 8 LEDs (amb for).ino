const byte led0 = 5;          // donar nom al pin 5 de lâ€™Arduino
//********** Setup ****************************************************************
void setup()
{
pinMode(led0, OUTPUT);
for(int i=0; i<30;i++)
 {
 delay(200);
  digitalWrite(led0, HIGH);    // posar a 5V el pin 5
 delay(100);
  digitalWrite(led0, LOW);    // posar a 0V el pin 5
 delay(100); 
 }
}
